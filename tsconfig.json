{
  "compilerOptions": {
    "target": "ESNext",
    "module": "esnext",
    "lib": [
      "esnext"
    ],
    "moduleResolution": "nodenext",
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true
  }
}
